{
  "private": true,
  "version": "1.1.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "node example/scripts/start.js",
    "build": "node scripts/build.js",
    "build:editor": "node scripts/build.editor.js",
    "build:language": "node scripts/build.language.js",
    "dts": "node scripts/dts.js",
    "test": "jest",
    "release": "node scripts/release.js",
    "format": "prettier --write --parser typescript \"packages/**/*.ts?(x)\"",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "build:antlr": "node scripts/build.antlr.js",
    "antlr": "antlr4 -Dlanguage=JavaScript ./src/language/antlr/GraphLexer.g4&antlr4 -Dlanguage=JavaScript ./src/language/antlr/GraphParser.g4",
    "language:debug": "node inspect -r ts-node/register ./packages/language/src/example/sample.ts",
    "editor:dev": "torch dev --config ./packages/editor/example/torch.config.ts",
    "editor:debug": "node inspect -r ts-node/register ./packages/editor/example/app.ts",
    "preinstall": "node ./scripts/checkYarn.js"
  },
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tqma113/graph-test-language.git"
  },
  "author": "Ma Tianqi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tqma113/graph-test-language/issues"
  },
  "homepage": "https://github.com/tqma113/graph-test-language#readme",
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint",
      "prettier --parser=typescript --write"
    ]
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-react": "^7.10.4",
    "@microsoft/api-extractor": "^7.9.11",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/jest": "^26.0.10",
    "@types/react": "^16.9.44",
    "antlr4": "^4.8.0",
    "chalk": "^4.1.0",
    "conventional-changelog-cli": "^2.1.0",
    "enquirer": "^2.3.6",
    "execa": "^4.0.3",
    "fs-extra": "^9.0.1",
    "jest": "^26.4.0",
    "minimist": "^1.2.5",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "rollup": "^2.23.0",
    "semver": "^7.3.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^8.10.2",
    "tslib": "^2.0.1",
    "typescript": "4.0.2"
  }
}
